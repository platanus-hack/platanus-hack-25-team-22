{
  "expo": {
    "name": "tiqn",
    "slug": "tiqn-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tiqnexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "owner": "kueks",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cl.tiqn.app",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show your position on the map and navigate to incidents.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to track your position during rescue operations.",
        "NSMicrophoneUsageDescription": "This app needs microphone access to enable voice conversations with the AI rescue assistant during emergency operations."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAVuPPVIwkNLd0jqXYg88Fhz_jz08H7O7M"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAVuPPVIwkNLd0jqXYg88Fhz_jz08H7O7M"
        }
      },
      "package": "cl.tiqn.app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "This app needs access to your location to track your position during rescue operations."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "821ced6a-292f-4d89-befd-c027df8f0cc8"
      }
    }
  }
}
